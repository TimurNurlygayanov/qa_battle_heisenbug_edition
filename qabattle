server {
    listen 80;
    
    root /var/www/qabattle;

    server_name qabattle.com;
    charset     utf-8;

    index index.html;

    location / {
        try_files $uri $uri/ =404;
    }

    location /auth {
        include         uwsgi_params;
        uwsgi_pass      unix:/qabattle/qabattle.sock;
    }
}
